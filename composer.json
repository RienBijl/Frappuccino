{
    "name": "rien/frappuccino",
    "description": "Lightweight PHP framework tailored to my personal needs",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Rien Bijl",
            "email": "helloitsrien@gmail.com"
        }
    ],
    "require": {
        "vlucas/phpdotenv": "^4.0"
    },
    "require-dev": {
      "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload" : {
        "psr-4": {
            "App\\": "app/",
            "Kernel\\": "kernel/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true
    }
}
